{
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch index.html",
      "file": "${workspaceFolder}/index.html",
      "webRoot": "${workspaceFolder}"
    },
     {
      "name": "Attach to Live Server",
      "type": "chrome",
      "request": "launch",
      "url": "http://127.0.0.1:5500",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
